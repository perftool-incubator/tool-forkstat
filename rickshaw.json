{
    "rickshaw-tool": {
        "schema": {
            "version": "2020.03.18"
        }
    },
    "tool": "forkstat",
    "controller": {
        "post-script": "%tool-dir%forkstat-post-process"
    },
    "collector": {
        "files-from-controller": [
            {
                "src": "%tool-dir%/forkstat-start",
                "dest": "/usr/bin/"
            },
            {
                "src": "%tool-dir%/forkstat-stop",
                "dest": "/usr/bin/"
            }
        ],
        "blacklist": [
            {
                "endpoint": "k8s",
                "collector-types": [ "client", "server" ]
            },
            {
                "endpoint": "kube",
                "collector-types": [ "client", "server" ]
            }
        ],
        "whitelist": [
            {
                "endpoint": "k8s",
                "collector-types": [ "master", "worker" ]
            },
            {
                "endpoint": "kube",
                "collector-types": [ "master", "worker" ]
            }
        ],
        "start": "forkstat-start",
        "stop": "forkstat-stop"
      }
}
